<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data></data>

    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="15dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:orientation="vertical">

            <GridLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:columnCount="3"
                android:orientation="vertical"
                android:rowCount="7">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/btn_all_studio_ca_text_0"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="1"
                        android:background="@drawable/all_ca_background_off"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="자연광"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/btn_all_studio_ca_text_1"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_marginStart="9dp"
                        android:layout_marginEnd="9dp"
                        android:layout_weight="1"
                        android:background="@drawable/all_ca_background_off"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="호리존"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/btn_all_studio_ca_text_2"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="1"
                        android:background="@drawable/all_ca_background_off"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="웨딩"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/btn_all_studio_ca_text_3"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="1"
                        android:background="@drawable/all_ca_background_off"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="파티"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/btn_all_studio_ca_text_4"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_marginStart="9dp"
                        android:layout_marginEnd="9dp"
                        android:layout_weight="1"
                        android:background="@drawable/all_ca_background_off"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="루프탑"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/btn_all_studio_ca_text_5"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="1"
                        android:background="@drawable/all_ca_background_off"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="키친"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/btn_all_studio_ca_text_6"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="1"
                        android:background="@drawable/all_ca_background_off"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="엔틱"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/btn_all_studio_ca_text_7"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_marginStart="9dp"
                        android:layout_marginEnd="9dp"
                        android:layout_weight="1"
                        android:background="@drawable/all_ca_background_off"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="컬러배경"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/btn_all_studio_ca_text_8"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="1"
                        android:background="@drawable/all_ca_background_off"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="빈티지"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/main_category"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/btn_all_studio_ca_text_9"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="1"
                        android:background="@drawable/all_ca_background_off"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="감성"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/btn_all_studio_ca_text_10"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_marginStart="9dp"
                        android:layout_marginEnd="9dp"
                        android:layout_weight="1"
                        android:background="@drawable/all_ca_background_off"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="컨셉별"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/btn_all_studio_ca_text_11"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="1"
                        android:background="@drawable/all_ca_background_off"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="셀프인물"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/btn_all_studio_ca_text_12"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="0.5"
                        android:background="@drawable/all_ca_background_off"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="야외촬영"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/btn_all_studio_ca_text_13"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_marginStart="9dp"
                        android:layout_weight="0.5"
                        android:background="@drawable/all_ca_background_off"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="일반 스튜디오"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_marginStart="9dp"
                        android:layout_weight="0.5"
                        android:background="@drawable/all_ca_background_off"
                        android:gravity="center"
                        android:visibility="invisible"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />
                </LinearLayout>



            </GridLayout>
        </LinearLayout>

    </androidx.coordinatorlayout.widget.CoordinatorLayout>

</layout>