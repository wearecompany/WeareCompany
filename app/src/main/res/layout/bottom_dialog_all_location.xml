<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data></data>

    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="15dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:orientation="vertical">

            <GridLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="25dp"
                android:columnCount="3"
                android:orientation="vertical"
                android:rowCount="6">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/btn_all_model_lo_text_0"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="1"
                        android:background="@drawable/join_background"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="서울"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/btn_all_model_lo_text_1"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_marginStart="9dp"
                        android:layout_marginEnd="9dp"
                        android:layout_weight="1"
                        android:background="@drawable/join_background"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="경기"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/btn_all_model_lo_text_2"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="1"
                        android:background="@drawable/join_background"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="인천"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/btn_all_model_lo_text_3"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="1"
                        android:background="@drawable/join_background"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="강원"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/btn_all_model_lo_text_4"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_marginStart="9dp"
                        android:layout_marginEnd="9dp"
                        android:layout_weight="1"
                        android:background="@drawable/join_background"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="제주"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/btn_all_model_lo_text_5"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="1"
                        android:background="@drawable/join_background"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="대전"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/btn_all_model_lo_text_6"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="1"
                        android:background="@drawable/join_background"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="충북"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/btn_all_model_lo_text_7"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_marginStart="9dp"
                        android:layout_marginEnd="9dp"
                        android:layout_weight="1"
                        android:background="@drawable/join_background"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="충남/세종"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/btn_all_model_lo_text_8"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="1"
                        android:background="@drawable/join_background"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="부산"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/main_category"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/btn_all_model_lo_text_9"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="1"
                        android:background="@drawable/join_background"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="울산"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/btn_all_model_lo_text_10"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_marginStart="9dp"
                        android:layout_marginEnd="9dp"
                        android:layout_weight="1"
                        android:background="@drawable/join_background"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="경남"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/btn_all_model_lo_text_11"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="1"
                        android:background="@drawable/join_background"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="대구"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/btn_all_model_lo_text_12"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="0.5"
                        android:background="@drawable/join_background"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="경북"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/btn_all_model_lo_text_13"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_marginStart="9dp"
                        android:layout_weight="0.5"
                        android:background="@drawable/join_background"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="광주"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/btn_all_model_lo_text_14"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_marginStart="9dp"
                        android:layout_weight="0.5"
                        android:background="@drawable/join_background"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="전남"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/btn_all_model_lo_text_15"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="0.5"
                        android:background="@drawable/join_background"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="전주/전북"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="0.5"
                        android:layout_marginStart="9dp"
                        android:visibility="invisible"
                        android:background="@drawable/join_background"
                        android:gravity="center"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_weight="0.5"
                        android:layout_marginStart="9dp"
                        android:visibility="invisible"
                        android:background="@drawable/join_background"
                        android:gravity="center"
                        android:textColor="#cbcbcb"
                        android:textSize="10dp" />


                </LinearLayout>


            </GridLayout>
        </LinearLayout>

    </androidx.coordinatorlayout.widget.CoordinatorLayout>

</layout>